<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aviumauctores.pioneers.controller.AddressInputScreenController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="%ip.address">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <TextField fx:id="addressTextField" prefWidth="200.0" text="localhost:27015">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="12.0" right="24.0" />
               </HBox.margin></TextField>
            <Button fx:id="setAddressButton" mnemonicParsing="false" onAction="#setAddress" text="%connect">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
